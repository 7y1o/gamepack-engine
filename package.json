{
  "name": "gamepack-engine",
  "version": "0.1.0",
  "private": true,
  "author": "7y1o",
  "scripts": {
    "serve": "vite preview",
    "build": "yarn vue:build && electron-builder",
    "dev": "concurrently -n Vue,Electron -c green,cyan \"yarn vue:dev\" \"yarn electron:dev\"",
    "electron:dev": "cross-env E_DEV=1 electron .",
    "vue:build": "vue-tsc --noEmit && vite build",
    "vue:dev": "vite",
    "vue:preview": "vite preview"
  },
  "main": "electron/main.js",
  "dependencies": {
    "@mdi/font": "5.9.55",
    "@types/three": "^0.141.0",
    "electron-dl": "^3.3.1",
    "element-plus": "^2.2.5",
    "equal-vue": "^0.79.3",
    "roboto-fontface": "*",
    "rxdb": "^12.4.3",
    "rxjs": "^7.5.5",
    "systeminformation": "^5.11.20",
    "three": "^0.141.0",
    "vue": "^3.2.25",
    "vuesax": "^4.0.1-alpha.25",
    "vuetify": "^3.0.0-beta.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@types/webfontloader": "^1.0.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "concurrently": "^7.2.1",
    "cross-env": "^7.0.3",
    "electron": "^19.0.4",
    "electron-builder": "^23.0.3",
    "sass": "^1.52.3",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vite-plugin-vuetify": "^1.0.0-alpha.0",
    "vue-cli-plugin-vuetify": "~2.5.1",
    "vue-tsc": "^0.34.7"
  },
  "build": {
    "appId": "ru.tdesu.gamepack",
    "productName": "GamePack Engine",
    "copyright": "Copyright Â© 2022. With love from 7y1o (https://vk.com/tdesu)",
    "files": [
      "./dist/**/*",
      "./electron/**/*"
    ],
    "fileAssociations": [
      {
        "ext": [
          "gpep"
        ],
        "name": "GamePack Engine Project",
        "icon": "./icons/gpep.ico"
      }
    ],
    "directories": {
      "output": "build"
    }
  },
  "license": "MIT"
}
